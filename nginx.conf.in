server {
    listen 80;
    server_name SERVER_NAME;

    location / {
        # used for frontend
        proxy_set_header   X-Forwarded-For $remote_addr;
        proxy_set_header   Host $http_host;
        proxy_pass         "http://127.0.0.1:5001";
    }

    location /convert {
        # used for backend
        proxy_set_header   X-Forwarded-For $remote_addr;
        proxy_set_header   Host $http_host;
        proxy_pass         "http://127.0.0.1:5002";
    }
}
